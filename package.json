{
  "name": "seven-gram",
  "type": "module",
  "version": "0.3.1",
  "description": "Telegram userbot written on Nodejs. Support popular miniapps like Hamster Kombat, Blum, TapSwap, Dogs, Major, OKX Racer and others.",
  "author": {
    "name": "pvmsnd",
    "email": "pvmnsd@gmail.com"
  },
  "license": "MIT",
  "main": "dist/main.js",
  "engines": {
    "node": "^22"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "postbuild": "tsc-alias",
    "build": "tsc",
    "build:watch": "concurrently \"tsc --watch\" \"tsc-alias --watch\"",
    "start": "NODE_NO_WARNINGS=1 node dist/main.js",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production && pm2 attach $(node -e \"console.log('$(pm2 id seven-gram)'[2])\")",
    "pm2:reload": "pm2 reload ecosystem.config.cjs --env production",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "@grammyjs/parse-mode": "^1.10.0",
    "async-retry": "^1.3.3",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "cron": "^3.1.7",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "humanize-duration": "^3.32.1",
    "lodash-es": "^4.17.21",
    "lowdb": "^7.0.1",
    "object-hash": "^3.0.0",
    "prompts": "^2.4.2",
    "telegraf": "^4.16.3",
    "telegram": "^2.25.11",
    "toad-scheduler": "^3.0.1",
    "zx": "^8.1.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@types/async-retry": "^1.4.8",
    "@types/humanize-duration": "^3.27.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.4",
    "@types/object-hash": "^3.0.6",
    "@types/prompts": "^2.4.9",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2"
  }
}
